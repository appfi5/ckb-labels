{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "$schema": {
      "type": "string"
    },
    "name": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "symbol": {
      "type": "string",
      "minLength": 1
    },
    "icon": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "decimal": {
      "type": "number",
      "minimum": 0
    },
    "tags": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "rgb++",
              "layer-1-asset",
              "layer-2-asset",
              "supply-limited"
            ]
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "manager": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "type": {
      "type": "object",
      "properties": {
        "codeHash": {
          "type": "string",
          "minLength": 1
        },
        "hashType": {
          "type": "string",
          "enum": [
            "type",
            "data",
            "data1",
            "data2"
          ]
        },
        "args": {
          "type": "string",
          "minLength": 1
        }
      },
      "required": [
        "codeHash",
        "hashType",
        "args"
      ],
      "additionalProperties": false
    },
    "typeHash": {
      "type": "string",
      "minLength": 1
    },
    "description": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "udtType": {
      "type": "string",
      "enum": [
        "sudt",
        "xudt",
        "xudt_compatible",
        "omiga_inscription",
        "ssri"
      ]
    },
    "published": {
      "type": "boolean"
    },
    "email": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "famous": {
      "type": "boolean"
    },
    "operatorWebsite": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "required": [
    "$schema",
    "name",
    "symbol",
    "icon",
    "decimal",
    "tags",
    "manager",
    "type",
    "typeHash",
    "description",
    "udtType",
    "published",
    "email",
    "famous",
    "operatorWebsite"
  ],
  "additionalProperties": false
}